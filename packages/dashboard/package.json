{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "start": "pnpm build && pnpm run server"
  },
  "dependencies": {
    "express": "^4.18.2",
    "vue": "^3.5.24",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.0.1",
    "cross-env": "^10.1.0",
    "tsx": "^4.7.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  }
}
