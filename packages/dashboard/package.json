    "start": "pnpm build && pnpm build:server && pnpm server"
  },
  "dependencies": {
    "vue": "^3.5.24",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "tsx": "^4.7.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  }
}
